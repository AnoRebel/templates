    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "eslint": "^8.2.0",
    "prettier": "^2.3.2",
    "eslint-config-prettier": "^8.3.0",
