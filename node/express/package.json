  "private": true,
  "scripts": {
    "start": "node --optimize_for_size --max_old_space_size=920 --gc_interval=100 ./bin/www",
    "dev": "NODE_ENV='development' nodemon --optimize_for_size --max_old_space_size=460 --gc_interval=100 ./bin/www"
  },
  "engines": {
    "yarn": "1.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anorebel/templates/node/express.git"
  },
